{
  "name": "@rwirnsberger/git-tools",
  "description": "A collection of git tools",
  "version": "1.0.1",
  "author": "Raphael Wirnsberger",
  "bin": {
    "git-tools": "./bin/run.js"
  },
  "bugs": "https://github.com/raphi-0901/git-tools/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "dayjs": "^1.11.18",
    "inquirer": "^12.9.6",
    "jira.js": "^5.2.2",
    "openai": "^5.23.0",
    "deepmerge-ts": "^7.1.5",
    "simple-git": "^3.28.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/raphi-0901/git-tools",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "git-tools",
    "dirname": "git-tools",
    "commands": "./dist/commands",
    "topicSeparator": " "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raphi-0901/git-tools.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
